{
  "name": "pump-dashboard",
  "version": "0.0.1",
  "scripts": {
    "clean": "rm -rf dist/*",
    "build:client": "node ./node_modules/webpack/bin/webpack.js --progress --colors",
    "build:server": "node ./node_modules/babel/bin/babel.js --stage=1 server -d dist && touch dist/logs && cp settings.json dist && cp schedule.json dist",
    "build": "npm run build:client && npm run build:server",
    "start": "npm run build && node dist",
    "package": "node pack.js",
    "test": "./node_modules/mocha/bin/mocha --compilers js:babel/register -t 10000 -c"
  },
  "dependencies": {
    "basic-auth": "^1.0.3",
    "body-parser": "^1.13.3",
    "chalk": "^1.1.1",
    "cookie-parser": "^1.3.5",
    "elemental": "^0.3.4",
    "express": "^4.13.3",
    "express-session": "^1.11.3",
    "hasher": "^1.2.0",
    "lodash": "^3.10.1",
    "morgan": "^1.6.1",
    "passport": "^0.3.0",
    "passport-local": "^1.0.0",
    "react": "^0.13.3",
    "react-loader": "^1.4.0",
    "request": "^2.65.0",
    "rx": "^3.1.1",
    "superagent": "^1.3.0"
  },
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-core": "^5.8.22",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.16.0",
    "file-loader": "^0.8.4",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "mocha": "^2.2.5",
    "node-sass": "^3.2.0",
    "react-hot-loader": "^1.2.9",
    "sass-loader": "^2.0.1",
    "style-loader": "^0.12.3",
    "supertest": "^1.0.1",
    "url-loader": "^0.5.6",
    "webpack": "^1.11.0",
    "webpack-dev-server": "^1.10.1"
  }
}
